@{
}
@model CurrencyApiResponse


@foreach(var item in Model.TCMB_AnlikKurBilgileri)
{
    <div class="listContainerThere"
         style="border-bottom: 5px solid #9cfc02;border-left: 5px solid #1dffec;border-top: 5px solid #9cfc02;border-right: 5px solid #1dffec;">


        <h2 class="listContainerTitle" style="font-family:fantasy;"> @item.Isim </h2>


        <div class="listContainerInThree">

            <div>

                <p class="lisContainerP" style="font-family:fantasy;color:#00930e;"> Alış Fiyatı : @item.ForexBuying</p>

                <p class="lisContainerP" style="font-family:fantasy;color:red;"> Satış Fiyatı : @item.ForexSelling</p>

            </div>
        </div>

    </div>
}